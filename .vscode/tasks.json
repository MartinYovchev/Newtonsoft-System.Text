{
    // FOR BUILD
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-newtonsoft",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/JsonComparison.Newtonsoft/JsonComparison.Newtonsoft.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-systemtext",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/JsonComparison.SystemText/JsonComparison.SystemText.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile"
    },
    {
      "label": "build-all",
      "command": "dotnet",
      "type": "process",
      "args": ["build", "${workspaceFolder}/JsonComparison.sln"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
